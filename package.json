  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.2",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "bootstrap": "^5.3.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^6.12.1",
    "react-scripts": "5.0.1",
    "react-snap-carousel": "^0.3.2",
    "styled-components": "^5.3.11",
    "styled-icons": "^10.47.0",
    "typescript": "^5.1.3",
    "web-vitals": "^3.3.2"
  },
  "scripts": {
    "lint": "eslint src/components/** src/App.t*",
    "lint:fix": "eslint src/components/** src/App.t* --fix",
    "lint:quiet": "eslint src/components/** src/App.t* --quiet",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "CI=true react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/node-logger": "^7.0.20",
    "@storybook/preset-create-react-app": "^7.0.20",
    "@storybook/react": "^7.0.20",
    "@types/node": "^20.3.1",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-plugin-react": "^7.32.2"
  }
